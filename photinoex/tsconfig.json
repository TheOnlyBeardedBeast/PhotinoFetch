{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./dist",
        "preserveConstEnums": true,
        "target": "es5",
        "lib": [
            "DOM",
            "ESNext"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}